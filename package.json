{
  "name": "@rosnik/browser-sdk",
  "version": "0.0.1",
  "description": "Description of your SDK",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "start": "node dist/index.js"
  },
  "keywords": [
    "sdk",
    "typescript",
    "javascript",
    "browser"
  ],
  "author": "Nick DiRienzo <nick@rosnik.ai>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^22.1.0",
    "prettier": "3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "^4.0.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "roots": [
      "<rootDir>/src",
      "<rootDir>/test"
    ],
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "verbose": false,
    "testRegex": "(/test/.*(test|spec))\\.ts?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
