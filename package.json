{
  "name": "@rosnik/browser-sdk",
  "version": "0.0.2",
  "description": "ROSNIK Browser SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublish": "rm -rf dist/* && tsc --project tsconfig.build.json",
    "build": "rm -rf dist/* && tsc --project tsconfig.build.json",
    "test": "jest",
    "start": "node dist/index.js",
    "pre:commit": ""
  },
  "keywords": [
    "sdk",
    "typescript",
    "javascript",
    "browser"
  ],
  "author": "Nick DiRienzo <nick@rosnik.ai>",
  "license": "MIT",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],  
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/js-cookie": "^3.0.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^22.1.0",
    "prettier": "3.0.3",
    "simple-git-hooks": "^2.9.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.0",
    "typescript": "^4.0.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "roots": [
      "<rootDir>/src",
      "<rootDir>/test"
    ],
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "verbose": false,
    "testRegex": "(/test/.*(test|spec))\\.ts?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "fetch-intercept": "^2.4.0",
    "js-cookie": "^3.0.5",
    "ulidx": "^2.2.0",
    "whatwg-fetch": "^3.6.19"
  }
}
